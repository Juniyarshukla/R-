#QUESTION.1
s1<-c(37,49,7,38)
s1
s2<-c(16,37,21,42,27,40,39,51)
s2
sp<-s1+s2
sp
sn<-(s1+s2)/2
sn
sd<-s1/s2
sd
sm<-s1*s2
sm
#--------------------------------------------------
#QUESTION.2
a<-seq(1,37,by=3)
a
b<-seq(1,13)
b
n1<-length(a)
n1
n2<-length(b)
n2
c<-a*b
c
d<-a/b
d
e<-a+b
e
f<-a-d
f
#--------------------------------------------------
#QUESTION.3
y<-c(40,67,75,48,44,53,56,43,66,57,65,52,83,83,80,76,85,88,89,87)
y
AM<-mean(y)  #TO FIND MEAN
AM
n<-length(y)
n
sm<-sum(y)
sm
am<-function(y){
  mean_value<-sum(y)/length(y)
  return(mean_value)
}
am
med<-median(y) #TO FIND MEDIAN
med
md<-function(y){ #TO FIND MODE
  tab<-table(y)
  ind<-tab[tab==max(tab)]
  mo<-as.numeric(names(ind))
  return(mo)
}
md
log10(y)      #TO FIND LOG BASE 10
log(y)        #TO FIND LOG
n<-length(y)  #TO FIND NO. OF OBSERVATION  
n
sr<-sort(y,decreasing = F) #TO SORT THE DATA
sr
sm<-summary(y)  #TO FIND SUMMARY OF DATA 
sm
ra<-max(y)-min(y) #TO FIND RANGE
ra
V<-var(y)       #TO FIND VARIANCE   
V
sd<-sqrt(V)     #TO FIND STANDARD DEVIATION      
sd
